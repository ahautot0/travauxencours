  3.6053754522857513`*^9, 3.605375457636561*^9}, {3.6053755045926433`*^9, 
  3.60537552569948*^9}, {3.6053755604563417`*^9, 3.6053755712827606`*^9}, {
  3.605426769783437*^9, 3.6054268043324127`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"dif", "[", "list_", "]"}], ":=", 
  RowBox[{"Prepend", "[", 
   RowBox[{
    RowBox[{"Differences", "[", 
     RowBox[{"PrimePi", "[", "list", "]"}], "]"}], ",", 
    RowBox[{
     RowBox[{"-", "1"}], "+", 
     RowBox[{"PrimePi", "[", 
      RowBox[{"list", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.605376300755642*^9, 3.605376303797647*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"primecode", "[", "integer_", "]"}], ":=", 
   RowBox[{"dif", "[", 
    RowBox[{"Flatten", "[", 
     RowBox[{"Replace", "[", 
      RowBox[{
       RowBox[{"FactorInteger", "[", "integer", "]"}], ",", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"a_Integer", ",", "b_Integer"}], "}"}], "->", 
        RowBox[{"Table", "[", 
         RowBox[{"a", ",", 
          RowBox[{"{", "b", "}"}]}], "]"}]}], ",", "1"}], "]"}], "]"}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.605250475818737*^9, 3.6052505023387833`*^9}, {
  3.6053756132156343`*^9, 3.605375629673663*^9}}],

